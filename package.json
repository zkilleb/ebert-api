    "@types/chai": "4.2.19",
    "@types/mocha": "8.2.2",
    "chai": "4.3.4",
    "jest": "27.0.5",
    "lint": "0.7.0",
    "prettier": "2.4.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "^4.5.0-dev.20210926"
  }
}
